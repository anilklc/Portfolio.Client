@model List<AdminGetInTouchDTO>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<div class="wrapper">
    <table>
        <thead>
            <tr>
                <th>Email</th>
                <th>Mobile</th>
                <th>Message</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Email</td>
                    <td>@item.Mobile</td>
                    <td>@item.Message</td>
                    <td><a class="btn update-btn" href="/Admin/Contact/UpdateContact/@item.Id">Update</a></td>
                    <td><a class="btn delete-btn" href="/Admin/Contact/DeleteContact/@item.Id">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
    <a class="btn create-btn" href="/Admin/Contact/CreateContact/">Create Contact</a>
</div>

