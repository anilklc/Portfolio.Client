@model List<AdminPartnerDTO>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_Layout.cshtml";
}

<div class="wrapper">
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Image</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td><img src="@item.ImageUrl" alt=""></td>
                    <td><a class="btn update-btn" href="/Admin/Partner/UpdatePartner/@item.Id">Update</a></td>
                    <td><a class="btn delete-btn" href="/Admin/Partner/DeletePartner/@item.Id">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
    <a class="btn create-btn" href="/Admin/Partner/CreatePartner/">Create Partner</a>
</div>


